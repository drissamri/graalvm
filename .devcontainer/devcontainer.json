{
  "name": "graalvm",
  "dockerFile": "Dockerfile",
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash"
  },
  "features": {
    "ghcr.io/devcontainers/features/node:1": {
      "version": "latest"
    }
  },
  "forwardPorts": [3000],
  "extensions": [
    "vscjava.vscode-java-pack", "dbaeumer.vscode-eslint"
  ],
  "runArgs": ["--init", "--privileged"],
  "overrideCommand": false,
}